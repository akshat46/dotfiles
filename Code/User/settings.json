        "list.inactiveSelectionForeground": "#8c3cfd",
        "list.highlightForeground": "#8c3cfd",
        "scrollbarSlider.activeBackground": "#8c3cfd50",
        "editorSuggestWidget.highlightForeground": "#8c3cfd",
        "textLink.foreground": "#8c3cfd",
        "progressBar.background": "#8c3cfd",
        "pickerGroup.foreground": "#8c3cfd",
        "tab.activeBorder": "#8c3cfd",
        "notificationLink.foreground": "#8c3cfd",
        "editorWidget.resizeBorder": "#8c3cfd",
        "editorWidget.border": "#8c3cfd",
        "settings.modifiedItemIndicator": "#8c3cfd",
        "settings.headerForeground": "#8c3cfd",
        "panelTitle.activeBorder": "#8c3cfd",
        "breadcrumb.activeSelectionForeground": "#8c3cfd",
        "menu.selectionForeground": "#8c3cfd",
